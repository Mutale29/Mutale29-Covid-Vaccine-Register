  
  
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Covid Vaccine Tracker</title>
    <link rel="stylesheet" href="css/normalize.css">
    <link href='https://fonts.googleapis.com/css?family=Nunito:400,300' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/styles/stylesheet.css">
</head>

<body>

  <form  method="post" onsubmit="return processFormSubmission();" >
  
    <h1>Welcome to our Covid Vaccine Tracker</h1>
    
      <h2>Blockchain Setup</h2>
      <button class="enableEthereumButton">Enable Ethereum</button> 
      <h2>Account: <span class="showAccount"></span></h2>
      <img src="\Images\Vaccine.jpg" alt="Vaccine">
      <h1> Covid Vaccine Register</h1>
      <h2 id="insertSm"></h2> <!--empty size 2 header container-->
          <p>Welcome to the Corona Virus Vaccine Register. Please fill out the form below.</p>
          <form id="info" action="/action_page.php">
              <label for="fname">First name:</label><br>
              <input type="text" id="fname" name="fname"><br>
              <label for="lname">Last name:</label><br>
              <input type="text" id="surname" name="lname"><br>
              <label for="date">Date of Vaccination</label><br>
              <input type="text" id="date_v" name="vdate"><br>
              <label for="loc">Vaccine Administered:</label><br>
              <input type="text" id="type_of_v" name="vname"><br> 
              <label for="loc">Place of vaccination:</label><br>
              <input type="text" id="place_of_v" name="vplace"><br>    
  
          </form>

  <br />

  <hr>


  <h2><%= status %></h1>

  <ul>
    <li>Item one</li>
    <li>Item two</li>
    <li>Item three</li>
    <li>Item four</li>
    <li>Item five</li>
  </ul>  

  <br />

  <ol>
    <li>Item one</li>
    <li>Item two</li>
    <li>Item three</li>
    <li>Item four</li>
    <li>Item five</li>
  </ol>  

    <ul>
      <% if ( typeof(users) =="undefined") { %>
        <li>No data</li> 
      <% } else { %> 
        <% for (let i = 0; i < users.length; i++) { %>
          <li><%= users[i] %> - <%= users[i].Name %>
             - <%= users[i].Surname %> 
             - <%= users[i].VaccineDate %>
             - <%= users[i].VaccineName %>
             - <%= users[i].VaccinePlace %>
            </li>
          <a  href="javascript:void(0);" class="btn btn-xs btn-success blockchain addVaccinationToBlockchainBtn"
                    data-fname="<%= users[i].Name%>"
                    data-lname="<%=users[i].Surname%>"
                    data-date="<%=users[i].VaccineDate%>"
                    data-type_of_v="<%=users[i].VaccineName%>"
                    data-place_of_v="<%=users[i].VaccinePlace%>">
                    Add to Blockchain</a> 
        <% } %>
       <% } %>
    
    </ul>

<script src="/scripts/main.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script> <!-- jQuery from Google Content Delivery Network (CDN)-->
<script src="https://cdn.ethers.io/scripts/ethers-v4.min.js" 
      charset="utf-8" type="text/javascript"></script> <!-- This exposes the library as a global variable: ethers -->
<script src="scripts/cvt_blockchain.js"></script>
</body>

</html>